{
  "manifest_version": 3,
  "name": "Clocky",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://*/*",  
    "http://*/*",
    "scripting"
    
  ],
    // Add other permissions as needed for your extension
  

  "action":{
    "default_icon":{
        "16": "icon-16.png",
        "32":"icon-32.png"},
    "default_title":"Clocky",
    "default_popup": "sites.html"
  }



}